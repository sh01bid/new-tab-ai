{
    "manifest_version": 3,
    "name": "New Tab AI",
    "version": "1.0.0",
    "description": "A new tab experience with AI chat",
    "permissions": [
        "storage",
        "topSites",
        "bookmarks"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}